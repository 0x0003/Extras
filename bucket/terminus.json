    "architecture": {
        "64bit": {
            "url": "https://github.com/Eugeny/terminus/releases/download/v1.0.96/terminus-1.0.96-setup.exe#/dl.7z",
            "hash": "sha512:1d245ef51abb089f883736a42836c790acdf74c7165e5ef6873bd3996c67bc67343925e607d37767dd5295bd4447245865ce08c234e6c006a232a8065f30e748",
            "installer": {
                "script": [
                    "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
