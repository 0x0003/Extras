            "hash": "f14983f9263599544e1b259c87e33e4895bde180cf0076f51cb114ef42b6948c"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/P6uVL0KShY7FpledzKtnNA_80.0.3971.0/80.0.3971.0_chrome_installer.exe#/dl.7z",
            "hash": "fd7bec92fb81d2ddfbc4c5398791549b1048b76577d98984d1ec00df7c17e2ee"
        }
    },
    "installer": {
        "script": "Expand-7zipArchive \"$dir\\chrome.7z\" -ExtractDir 'Chrome-bin' -Removal"
