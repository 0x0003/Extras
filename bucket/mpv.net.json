{
    "homepage": "https://github.com/stax76/mpv.net/",
    "description": "A modern media player for Windows that works just like mpv",
    "license": "MIT",
    "version": "4.2",
    "architecture": {
        "64bit": {
            "url": "https://github.com/stax76/mpv.net/releases/download/4.2/mpv.net-portable-x64-4.2.0.0.7z",
            "hash": "fa5f70c0e06570d8710e1e2ff553ec371edd502f66a99fe193e0098ab68b2603"
        },
        "32bit": {
            "url": "https://github.com/stax76/mpv.net/releases/download/4.2/mpv.net-portable-x86-4.2.0.0.7z",
            "hash": "f8c0beb4730e188030ebd41b88e272d94d5ad255ee47d1554fbf2fa74b2726bd"
        }
    },
    "bin": "mpvnet.exe",
    "shortcuts": [
        [
            "mpvnet.exe",
            "mpv.net"
        ]
    ],
    "persist": "portable_config",
    "checkver": {
        "github": "https://github.com/stax76/mpv.net",
        "regex": "download/(?<main>[\\d.]+)/mpv.net-portable-x64-(?<full>[\\d.]+)\\.7z",
        "replace": "${main}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/stax76/mpv.net/releases/download/$matchMain/mpv.net-portable-x64-$matchFull.7z"
            },
            "32bit": {
                "url": "https://github.com/stax76/mpv.net/releases/download/$matchMain/mpv.net-portable-x86-$matchFull.7z"
            }
        }
    }
}
