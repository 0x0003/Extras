{
    "homepage": "https://github.com/stax76/mpv.net/",
    "description": "A modern media player for Windows that works just like mpv",
    "license": "MIT",
    "version": "4.3",
    "architecture": {
        "64bit": {
            "url": "https://github.com/stax76/mpv.net/releases/download/4.3/mpv.net-portable-x64-4.3.0.0.7z",
            "hash": "b8ca1f507b2e5c47a373b85c307e2f99c4ad888df2c7cf81cf4496c728589334"
        },
        "32bit": {
            "url": "https://github.com/stax76/mpv.net/releases/download/4.3/mpv.net-portable-x86-4.3.0.0.7z",
            "hash": "47485ca9feecfd06f074ac2d00a29460e8af9d94ca555e1ea8b454820cd3f754"
        }
    },
    "bin": "mpvnet.exe",
    "shortcuts": [
        [
            "mpvnet.exe",
            "mpv.net"
        ]
    ],
    "persist": "portable_config",
    "checkver": {
        "github": "https://github.com/stax76/mpv.net",
        "regex": "download/(?<main>[\\d.]+)/mpv.net-portable-x64-(?<full>[\\d.]+)\\.7z",
        "replace": "${main}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/stax76/mpv.net/releases/download/$matchMain/mpv.net-portable-x64-$matchFull.7z"
            },
            "32bit": {
                "url": "https://github.com/stax76/mpv.net/releases/download/$matchMain/mpv.net-portable-x86-$matchFull.7z"
            }
        }
    }
}
