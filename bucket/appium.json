    "version": "1.13.0",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/appium/appium-desktop/releases/download/v1.13.0/Appium-windows-1.13.0.exe#/dl.7z",
            "hash": "sha512:d4ef2f84581444f7b4ba79398dedd0815509310233c2399a16c820cfcc2ca092bfd02ab5dd112c191c317f1093e139cc6ca286523eed2a271f23f6320869bf7b",
            "installer": {
                "script": [
                    "extract_7zip \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                    "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\Uninst*\" -Force -Recurse"
                ]
            }
        }
    },
    "shortcuts": [
        [
            "Appium.exe",
            "Appium"
        ]
    ],
    "checkver": {
        "github": "https://github.com/appium/appium-desktop"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/appium/appium-desktop/releases/download/v$version/Appium-windows-$version.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "find": "sha512:\\s+(.*)"
                }
            }
        }
    }
}
